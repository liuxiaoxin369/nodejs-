		<div class="container">
			<div class="starter-template">
				<h1>查询用户信息</h1>
				<form role="form" class="form-inline">
					<!--/form-->
					<table class="table table-striped table-bordered table-hover table-condensed" id="tablepeople">
						<thead>
							<tr>
								<th>用户名</th>
								<th>密码</th>
								<th>部门</th>
                <th>操作</th>
							</tr>
						</thead>
						<tbody>
              <% for(var i = 0; i < peopleArr.length; i++) { %>
                <tr>
                  <td><%= (peopleArr[i]).username %></td>
                  <td><%= (peopleArr[i]).password %></td>
                  <td><%= (peopleArr[i]).role %></td>
                  <td><a href="/deletePeople?username=<%= (peopleArr[i]).username %>">删除</a></td>
                </tr>
              <%}%>
						</tbody>
					</table>
				</form>
